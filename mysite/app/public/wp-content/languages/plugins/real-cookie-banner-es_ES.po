msgctxt "legal-text"
msgid "[rcb-consent type=\"history\" tag=\"a\" text=\"Privacy settings history\"]"
msgstr "[rcb-consent type=\"historial\" tag=\"a\" text=\"Historial de configuración de privacidad\"]"

