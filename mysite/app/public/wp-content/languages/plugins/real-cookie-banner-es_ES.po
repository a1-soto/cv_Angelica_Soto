msgctxt "legal-text"
msgid "[rcb-consent type=\"revoke\" tag=\"a\" text=\"Revoke consents\" successmessage=\"You have successfully revoked consent for services with its cookies and personal data processing. The page will be reloaded now!\"]"
msgstr "[rcb-consent type=\"revocar\" tag=\"a\" text=\"Revocar consentimientos\" successmessage=\"Ha revocado correctamente el consentimiento para servicios con sus cookies y procesamiento de datos personales. ¡La página se actualizará ahora!\"]"

